{
  "name": "replace-last",
  "version": "1.1.0",
  "description": "Template for creating an npm package in 2018",
  "author": "danday74",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover -x \"**/*.spec.js\" ./node_modules/mocha/bin/_mocha -- js",
    "enforcer": "istanbul check-coverage --statement 100 --branch 100 --function 100 --line 100",
    "build": "npm test && npm run enforcer",
    "prepush": "npm run build"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "2.11.16",
    "eslint": "^4.19.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.4"
  },
  "keywords": [
    "replace",
    "last"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danday74/node-template-18.git"
  },
  "bugs": {
    "url": "https://github.com/danday74/node-template-18/issues"
  },
  "homepage": "https://github.com/danday74/node-template-18#readme",
  "comment": "coveralls@2.11.16"
}
